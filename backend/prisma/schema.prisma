generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id Int @id @default(autoincrement())
  title String @unique
  description String
  status String
  createdAt DateTime @default(now())

  @@map("tasks")
}